{
  "name": "docs.escaladesports.com",
  "version": "0.0.0",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "scripts": {
    "build": "npm run patch && NODE_ENV=production gatsby build",
    "clean": "run-p clean:gatsbyCache clean:nodeCache",
    "clean:gatsbyCache": "rm -rf .cache",
    "clean:nodeCache": "rm -rf node_modules/.cache",
    "dev": "npm run clean:nodeCache &&  npm run dev:gatsby",
    "dev:gatsby": "gatsby develop --open",
    "encrypt": "envdotjs encrypt",
    "lint": "eslint ./ --fix",
    "live": "opn https://escalade-docs.netlify.com/",
    "netlify": "opn https://app.netlify.com/sites/escalade-docs/deploys",
    "patch": "sed -i -e 's/if(c\\.initial) return;/if(!c||c.initial) return;/g' ./node_modules/extract-text-webpack-plugin/index.js",
    "serve": "cd public && serve --open",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint ./ --fix",
      "git add"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "eslint-config-escalade-sports": "^0.1.2",
    "gatsby": "^1.9.247",
    "gatsby-image": "^1.0.51",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-esca-css": "^1.0.3",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-html-attributes": "^1.0.4",
    "gatsby-plugin-manifest": "^1.0.22",
    "gatsby-plugin-markdown-pages": "^1.0.2",
    "gatsby-plugin-netlify": "^1.0.19",
    "gatsby-plugin-netlify-cache": "^0.1.0",
    "gatsby-plugin-netlify-cms": "^2.0.1",
    "gatsby-plugin-netlify-identity-widget": "^1.1.0",
    "gatsby-plugin-offline": "^1.0.16",
    "gatsby-plugin-polyfill-io": "^1.0.3",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-remove-trailing-slashes": "^1.0.9",
    "gatsby-plugin-sharp": "^1.6.44",
    "gatsby-plugin-sitemap": "^1.2.25",
    "gatsby-plugin-web-font-loader": "^1.0.2",
    "gatsby-remark-copy-linked-files": "^1.5.32",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-prismjs": "^2.0.2",
    "gatsby-remark-smartypants": "^1.4.12",
    "gatsby-source-filesystem": "^1.5.36",
    "gatsby-transformer-remark": "^1.7.41",
    "gatsby-transformer-sharp": "^1.6.24",
    "npm-run-all": "^4.1.3",
    "opn-cli": "^3.1.0",
    "prism-themes": "^1.0.1",
    "prismjs": "^1.14.0",
    "react": "^16.4.0",
    "react-burgers": "^1.0.4",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-no-ssr": "^1.1.0",
    "statable": "^0.3.2",
    "static-server": "^2.2.1"
  }
}
