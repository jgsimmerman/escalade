		"filestack-js": "^0.9.5",
		"form-serialize": "^0.7.2",
		"front-matter": "^2.2.0",
		"fs-extra": "^4.0.0",
		"glob-all": "^3.1.0",
		"glob-promise": "^3.1.0",
		"globby": "^6.1.0",
		"google-map-react": "^0.24.0",
		"googleapis": "^21.3.0",
		"gray-matter": "^3.0.2",
		"html-minifier": "^3.5.2",
		"is-email": "^1.0.0",
		"is-phone": "^2.0.0",
		"is-valid-zip": "^0.0.3",
		"isomorphic-fetch": "^2.2.1",
		"json2yaml": "^1.1.0",
		"lost": "^8.1.0",
		"markdown-to-json-files": "^1.1.1",
		"marked": "^0.3.6",
		"minimatch": "^3.0.4",
		"minimist": "^1.2.0",
		"next": "beta",
		"npm-run-all": "^4.0.2",
		"open": "^0.0.5",
		"portscanner": "^2.1.1",
		"postcss-cssnext": "^2.11.0",
		"postcss-easy-import": "^2.1.0",
		"postcss-loader": "^2.0.6",
		"privacy-policy": "escaladesports/privacy-policy#v1.0.3",
		"product-to-json": "escaladesports/product-to-json#v0.1.0",
		"query-product": "escaladesports/query-product#v1.0.3",
		"raw-loader": "^0.5.1",
		"react": "^15.6.1",
		"react-cursor-position": "^2.2.1",
		"react-dom": "^15.2.1",
