{
  "name": "gatsby-boilerplate",
  "version": "1.0.0",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "scripts": {
    "build": "run-p build:gatsby build:functions",
    "build:gatsby": "NODE_ENV=production gatsby build",
    "build:functions": "netlify-lambda build src/functions",
    "clean": "run-p clean:gatsbyCache clean:nodeCache",
    "clean:gatsbyCache": "rm -rf .cache",
    "clean:nodeCache": "rm -rf node_modules/.cache",
    "dev": "run-p dev:functions dev:gatsby",
    "dev:gatsby": "gatsby develop --open",
    "dev:functions": "netlify-lambda serve src/functions",
    "env": "node utils/env",
    "lint": "eslint ./src",
    "live": "opn https://gatsby-boilerplate.netlify.com/",
    "netlify": "opn https://app.netlify.com/sites/gatsby-boilerplate/deploys",
    "precommit": "lint-staged",
    "reset": "node utils/reset",
    "serve": "gatsby serve --open",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "child-process-promise": "^2.2.1",
    "color": "^3.0.0",
    "dotenv": "^6.0.0",
    "emotion": "^9.2.6",
    "emotion-server": "^9.2.6",
    "eslint": "^5.2.0",
    "eslint-config-escalade-sports": "^0.2.3",
    "fs-extra": "^7.0.0",
    "gatsby": "^1.9.247",
    "gatsby-image": "^1.0.51",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-canonical-urls": "^1.0.18",
    "gatsby-plugin-emotion": "^1.1.17",
    "gatsby-plugin-esca-css": "^1.0.3",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-html-attributes": "^1.0.4",
    "gatsby-plugin-manifest": "^1.0.22",
    "gatsby-plugin-markdown-pages": "^1.0.2",
    "gatsby-plugin-netlify": "^1.0.19",
    "gatsby-plugin-netlify-cms": "^2.0.1",
    "gatsby-plugin-netlify-cms-paths": "^1.0.1",
    "gatsby-plugin-netlify-identity-widget": "^1.1.0",
    "gatsby-plugin-offline": "^1.0.16",
    "gatsby-plugin-polyfill-io": "^1.0.3",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-recaptcha": "^1.0.4",
    "gatsby-plugin-remove-trailing-slashes": "^1.0.9",
    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sharp": "^1.6.44",
    "gatsby-plugin-sitemap": "^1.2.25",
    "gatsby-plugin-web-font-loader": "^1.0.3",
    "gatsby-remark-copy-linked-files": "^1.5.32",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^1.5.67",
    "gatsby-remark-smartypants": "^1.4.12",
    "gatsby-source-filesystem": "^1.5.36",
    "gatsby-transformer-remark": "^1.7.41",
    "gatsby-transformer-sharp": "^1.6.24",
    "http-proxy-middleware": "^0.18.0",
    "husky": "^0.14.3",
    "isomorphic-fetch": "^2.2.1",
    "lint-staged": "^7.2.0",
    "netlify-cms": "^1.9.3",
    "netlify-lambda": "^0.4.0",
    "npm-run-all": "^4.1.3",
    "opn-cli": "^3.1.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-netlify-form": "^1.3.0",
    "react-no-ssr": "^1.1.0",
    "react-recaptcha": "^2.3.10",
    "statable": "^0.3.2",
    "static-server": "^2.2.1",
    "toml": "^2.3.3"
  }
}
