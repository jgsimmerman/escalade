{
	"name": "boilerplate",
	"version": "0.0.0",
	"main": "index.js",
	"repository": "git@github.com:escaladesports/boilerplate.git",
	"author": "Kennedy Rose <kennedy@escaladesports.com>",
	"license": "MIT",
	"scripts": {
		"dev": "npm-run-all --parallel watch server",
		"server": "node server.js",
		"copy": "node utils/copy",
		"prebuild": "npm run clean && npm-run-all --parallel markdown privacy-policy terms-of-service salsify img && npm run merge-product-data && npm run all-json",
		"build": "next build && next export -o dist",
		"buildtest": "npm run build-test",
		"testbuild": "npm run build-test",
		"test-build": "npm run build-test",
		"build-test": "npm run build && npm run serve-static",
		"postbuild": "npm-run-all --parallel sitemap copy minify",
		"img": "node -e \"require('./utils/images')()\"",
		"youtube": "node utils/scrape-youtube",
		"watch": "node utils/watch",
		"sitemap": "node utils/sitemap",
		"markdown": "node utils/markdown",
		"all-json": "node utils/all-json",
		"minify": "html-minifier --input-dir ./dist --output-dir ./dist --file-ext html --config-file ./config/.htmlmin",
		"privacy-policy": "node utils/markdown-modules -m privacy-policy",
		"terms-of-service": "node utils/markdown-modules -m terms-of-service",
		"clean": "node utils/clean",
		"salsify": "node utils/salsify",
		"merge-product-data": "node utils/merge-product-data",
		"replace-images": "node utils/replace-images",
		"serve-static": "node utils/serve-static",
		"edit": "node utils/markdown-edit",
		"salsify-check": "yarn salsify && node utils/salsify-check",
		"test": "yarn replace-images"
	},
	"devDependencies": {
		"autoprefixer": "^7.1.1",
		"babel-plugin-dotenv": "^0.0.2",
		"babel-plugin-inline-react-svg": "^0.4.0",
		"babel-plugin-module-resolver": "^2.7.1",
		"babel-plugin-styled-components": "^1.2.0",
		"babel-plugin-transform-inline-environment-variables": "^0.1.1",
		"babel-plugin-wrap-in-js": "^1.1.1",
		"child-process-promise": "^2.2.1",
		"chokidar": "^1.7.0",
		"class-list": "^0.1.1",
		"country-list": "^1.1.0",
		"cpy": "^5.0.0",
		"css-mqpacker": "^6.0.1",
		"cssnano": "^3.10.0",
		"datasets-us-states-abbr-names": "^1.0.0",
		"datasets-us-states-names": "^1.0.0",
		"detectie": "^1.0.0",
