{
  "name": "esca-scripts",
  "version": "1.1.2",
  "main": "dist/cli.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/esca-scripts.git"
  },
  "license": "MIT",
  "bin": {
    "esca-scripts": "dist/index.js"
  },
  "scripts": {
    "build:test": "parcel build src-test/component.js --global Component --no-minify --no-cache",
    "build": "babel-node src build",
    "bundle": "babel-node src bundle --src src-test/component.js --dist dist-bundle-test",
    "dev": "babel-node src dev",
    "help": "babel-node src --help",
    "run": "babel-node src run --file src-test/run.js",
    "serve": "babel-node src serve --dir dist-dev",
    "test": "npm run build && babel-node src test",
    "travis": "opn https://travis-ci.org/escaladesports/esca-scripts/branches"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-shebang": "^0.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "child-process-promise": "^2.2.1",
    "css-mqpacker": "^6.0.2",
    "fs-extra": "^6.0.0",
    "get-port": "^3.2.0",
    "jest": "^22.4.3",
    "lost": "^8.2.1",
    "parcel-bundler": "^1.8.1",
    "parcel-plugin-bundle-visualiser": "^1.1.2",
    "postcss-nested": "^3.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "source-map-explorer": "^1.5.0",
    "static-server": "^2.2.1",
    "styled-jsx": "^2.2.6",
    "styled-jsx-plugin-postcss": "^0.1.3",
    "subcommander": "^1.0.0",
    "webpack": "^4.8.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "devDependencies": {
    "opn-cli": "^3.1.0",
    "puppeteer": "^1.3.0",
    "react-test-renderer": "^16.3.2"
  }
}
