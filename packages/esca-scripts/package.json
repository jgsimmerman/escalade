{
  "name": "esca-scripts",
  "version": "0.1.4",
  "main": "dist/cli.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/esca-scripts.git"
  },
  "license": "MIT",
  "scripts": {
    "analyze": "babel-node src/cli analyze --input './src/cli.js'",
    "build": "babel-node src/cli build --input './src/cli.js' --cli",
    "help": "babel-node src/cli --help",
    "prepublishOnly": "npm run test && npm run build",
    "serve": "babel-node src/cli serve",
    "test": "npm run build && mocha --require babel-core/register"
  },
  "bin": {
    "esca-scripts": "dist/cli.js"
  },
  "pre-commit": [
    "test"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.1",
    "fs-extra": "^5.0.0",
    "mocha": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "static-server": "escaladesports/static-server",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "dependencies": {
    "exeq": "^3.0.0",
    "meow": "^4.0.0",
    "webpack-node-externals": "^1.6.0"
  }
}
