{
  "name": "esca-scripts",
  "version": "0.9.9",
  "main": "dist/cli.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/esca-scripts.git"
  },
  "license": "MIT",
  "scripts": {
    "analyze": "babel-node src/cli analyze --input './src/cli.js'",
    "build": "babel-node src/cli build --input './src/cli.js' --cli",
    "build:browser": "babel-node src/cli build --input './dev/browser.js' --output './dist/browser.js' --component --name TestComponent --browser --minify false",
    "build:react": "babel-node src/cli build ---input ./dev/react-component.js --output ./dist/react.js --react --minify false --component",
    "dev": "babel-node src/cli dev ---input './dev/react.js, ./dev/index.html' --react --browser",
    "dev:react": "babel-node src/cli dev ---input './dev/react.js, ./dev/index.html' --react",
    "help": "babel-node src/cli --help",
    "open": "opn https://travis-ci.org/escaladesports/esca-scripts/branches",
    "prepublishOnly": "npm run build",
    "reset": "babel-node src/cli reset",
    "serve": "babel-node src/cli serve --dir dev",
    "test": "npm run build && node dist/cli test"
  },
  "bin": {
    "esca-scripts": "dist/cli.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-regenerator-runtime": "^6.5.0",
    "chai": "^4.1.2",
    "child-process-promise": "^2.2.1",
    "commander": "^2.13.0",
    "cross-env": "^5.1.1",
    "css-mqpacker": "^6.0.2",
    "cssnano": "^3.10.0",
    "fs-extra": "^5.0.0",
    "git-rev-promises": "^1.0.8",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "json2yaml": "^1.1.0",
    "lost": "^8.2.0",
    "meow": "^4.0.0",
    "mocha": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "opn": "^5.2.0",
    "opn-cli": "^3.1.0",
    "portfinder": "^1.0.13",
    "postcss-cssnext": "^3.1.0",
    "postcss-nested": "^3.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "reload-html-webpack-plugin": "^0.1.2",
    "static-server": "escaladesports/static-server",
    "styled-jsx": "^2.2.3",
    "styled-jsx-plugin-postcss": "^0.1.2",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.11.1",
    "webpack-node-externals": "^1.6.0",
    "write-yaml": "^1.0.0",
    "yn-cli": "^1.0.0"
  },
  "devDependencies": {
    "js-cookie": "^2.2.0"
  }
}
