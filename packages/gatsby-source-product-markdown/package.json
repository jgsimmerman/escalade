{
  "name": "gatsby-source-product-markdown",
  "version": "1.0.8",
  "keywords": [
    "gatsby",
    "plugin",
    "source",
    "product",
    "markdown"
  ],
  "main": "gatsby-node.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/gatsby-source-product-markdown.git"
  },
  "license": "MIT",
  "scripts": {
    "analyze": "esca-scripts analyze --input './src/gatsby-browser.js'",
    "build": "esca-scripts build --input src --output . --multiple --react --babel",
    "prepublishOnly": "npm run build",
    "reset": "esca-scripts reset",
    "test": "npm run build && esca-scripts test",
    "travis": "opn https://travis-ci.org/escaladesports/gatsby-source-product-markdown/branches"
  },
  "devDependencies": {
    "esca-scripts": "^0.14.1",
    "opn-cli": "^3.1.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "front-matter": "^2.3.0",
    "globby": "^8.0.1",
    "read-markdown": "^1.0.2"
  }
}
