{
  "name": "zygote-cart",
  "version": "1.0.2",
  "keywords": ["zygote", "react", "cart"],
  "main": "dist/index.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/react-component-boilerplate.git"
  },
  "license": "MIT",
  "scripts": {
    "analyze":
      "esca-scripts analyze --react --component --input ./src/index.js",
    "build":
      "esca-scripts build --react --component --babel --input src --output dist --multiple --styledJsx",
    "dev":
      "esca-scripts dev ---input './dev/dev.js, ./dev/index.html' --react --browser --styledJsx",
    "prepublishOnly": "npm run build",
    "reset": "esca-scripts reset",
    "test": "echo 'no test'",
    "travis":
      "opn https://travis-ci.org/escaladesports/react-component-boilerplate/branches"
  },
  "devDependencies": {
    "esca-scripts": "^0.14.12",
    "opn-cli": "^3.1.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "js-cookie": "^2.2.0",
    "statable": "^0.2.3",
    "styled-components": "^3.2.5",
    "styled-jsx": "^2.2.6",
    "validator": "^9.4.1"
  }
}
