  "dependencies": {
    "camelcase": "^4.1.0",
    "crypto": "^1.0.1",
    "es6-error": "^4.0.2",
    "front-matter": "^2.3.0",
    "fs-extra": "^5.0.0",
    "globby": "^7.1.1",
    "graphql-client": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.4",
    "read-pkg-up": "^2.0.0"
