{
  "name": "gatsby-source-salsify",
  "version": "1.0.5",
  "description": "Fetch data from Salsify API",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "format": "prettier --write 'src/**/*.js' README.md",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "watch": "npm run build -- --watch"
  },
  "homepage": "https://github.com/escaladesports/gatsby-source-salsify",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/gatsby-source-salsify.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "salsify"
  ],
  "author": "Kennedy Rose <kennedy@escaladesports.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "gatsby": "^1.9.36",
    "gatsby-node-helpers": "^0.1.1",
    "graphql-tools": "^1.2.2",
    "jest": "^21.1.0",
    "joi": "^11.0.1",
    "prettier": "^1.9.2"
