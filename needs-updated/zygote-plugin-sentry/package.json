{
  "name": "@escaladesports/zygote-plugin-sentry",
  "version": "0.0.0",
  "description": "Zygote Sentry Reporting Plugin",
  "main": "dist/index.js",
  "scripts": {
    "analyze": "esca-scripts analyze --react --component --input ./src/index.js",
    "build": "esca-scripts build --react --component --babel --input src --output dist --multiple",
    "dev": "esca-scripts dev ---input './dev/dev.js, ./dev/index.html' --react --browser",
    "prepublishOnly": "npm run build",
    "reset": "esca-scripts reset",
    "test": "echo 'no test'"
  },
  "repository": "git@github.com:escaladesports/zygote-plugin-sentry.git",
  "author": "Matt Suhay <matthew.suhay@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "esca-scripts": "^1.5.4",
    "react": "^16.8.4",
    "react-async-script-loader": "^0.3.0",
    "react-dom": "^16.8.4",
    "statable": "^0.3.2"
  },
  "peerDependencies": {
    "@escaladesports/zygote-cart": "^3.0.0"
  }
}
