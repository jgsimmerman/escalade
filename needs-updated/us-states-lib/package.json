  "license": "MIT",
  "keywords": [
    "us",
    "states",
    "lib"
  ],
  "repository": {
    "git": "git",
    "url": "git+https://github.com/escaladesports/us-states-lib.git"
  },
  "bugs": {
    "url": "https://github.com/escaladesports/us-states-lib/issues"
  },
  "homepage": "https://github.com/escaladesports/us-states-lib#readme",
  "dependencies": {},
  "devDependencies": {
    "esca-scripts": "^1.5.4",
    "eslint-config-escalade-sports": "^1.0.2"
  }
}
